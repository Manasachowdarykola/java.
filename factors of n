import java.util.*;
class factor
{
public static void main(String []args)
{
try
{
Scanner Sc=new Scanner(System.in);
System.out.println("enter the number:");
 int n=Sc.nextInt();
int i,count=0;
for(i=1;1<=n;i++)
{
if(n%i==0)

count++;
}
System.out.println("the factors of "+n+"is "+count);
}
catch (Exception e)
{
System.out.println("invlid due to negitive numbers");
}
finally
{
System.out.println("output verified");
}
}
}
